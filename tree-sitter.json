{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "requirements",
      "title": "pip requirements",
      "scope": "source.requirements",
      "file-types": [
        "requirements.txt",
        "pip"
      ],
      "injection-regex": "^requirements$",
      "highlights": "queries/highlights.scm"
    }
  ],
  "metadata": {
    "version": "0.4.0",
    "description": "pip requirements grammar for tree-sitter",
    "authors": [
      {
        "name": "ObserverOfTime",
        "email": "chronobserver@disroot.org",
        "url": "https://observeroftime.github.io/"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter-grammars/tree-sitter-requirements",
      "funding": "https://github.com/sponsors/ObserverOfTime"
    }
  }
}
